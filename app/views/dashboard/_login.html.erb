<div style="background-color: #6c757d">
  <% if current_user %>
    Logged in as <%= current_user.email %>.

    <%= link_to "Log Out", logout_path %>
  <% else %>
  <div class="login">
    <%= link_to image_tag('youngcapital.png', alt: "Inloggen", style: "height: 100px; margin-right: 2em;"), login_path %> OR
    <%= link_to image_tag('register.jpg', alt: "Registeren", style: "height: 100px; margin-left: 2em;"), signup_path %>
  </div
  <% end %>
</div>
