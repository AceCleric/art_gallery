<p id="notice"><%= notice %></p>
<% tab "user" %>
<%= render 'dashboard/navbar' %>

<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %>

<% if !current_user %>
  <%= link_to 'Back', users_path %>
<% end %>
